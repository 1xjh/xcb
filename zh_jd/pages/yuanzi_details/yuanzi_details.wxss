/* zh_jd/pages/yuanzi_details/yuanzi_details.wxss */

/* 轮播 */

.swiper {
  width: 100%;
  height: 450rpx;
  background: #f7f6f6;
}

.yz_banner {
  width: 100%;
  height: 100%;
}

.yz_banner image {
  width: 100%;
  height: 100%;
}

.box {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

.xian {
  width: 100%;
  height: 1px;
  background: #f7f6f6;
}

/* 房屋类型 */

.yz_type {
  width: 90%;
  margin: 0 auto;
  justify-content: space-between;
  padding: 20rpx 0;
}

.yz_type_title text {
  font-size: 35rpx;
}

.yz_type_title view {
  font-size: 23rpx;
}

.yz_type_title view image {
  width: 42rpx;
  height: 35rpx;
  margin-right: 7rpx;
  float: left;
}

.yz_icon image {
  width: 33rpx;
  height: 29rpx;
  margin-left: 6rpx;
}

.yz_icon {
  font-size: 23rpx;
  margin-top: 20rpx;
  color: #40ab91;
  background-color: #fff;
  border-radius: 0;
  height: 70rpx;
  line-height: 35rpx;
}

/* 优惠 */

.yz_discounts {
  font-size: 20rpx;
  padding: 20rpx 0;
  line-height: 32rpx;
}

.yz_discounts view {
  margin-left: 44rpx;
  color: #40ab91;
}

.yz_discounts view:first-child {
  margin-left: 44rpx;
  background: #40ab91;
  color: #fff;
  width: 100rpx;
  height: 32rpx;
  text-align: center;
  border-radius: 16rpx;
}

/* 底下内容背景色 */

.content_color {
  width: 100%;
  height: 100%;
  background: #f7f6f6;
}

/* 图标内容 */

.yz_icon_title {
  justify-content: space-around;
  width: 100%;
  padding: 10rpx 0;
  flex: 1;
  background-color: #fff;
}

.yz_icon_title1 {
  padding: 30rpx 0;
  justify-content: space-around;
  width: 100%;
  background-color: #f7f6f6;
}

.yz_icon_content {
  flex-direction: column;
  align-items: center;
}

.yz_icon_content image {
  width: 60rpx;
  height: 60rpx;
}

.yz_icon_content view {
  font-size: 16rpx;
}

.yz_icon_content text {
  display: block;
  font-size: 24rpx;
}

/* 入离时间 */

.residence_box {
  display: flex;
  justify-content: space-around;
  align-items: center;
  text-align: center;
  padding: 12rpx 0;
  font-size: 30rpx;
}

.residence_box text {
  font-size: 24rpx;
  color: rgba(95, 95, 95, 1);
}

.search_box {
  display: flex;
  padding: 12rpx 0;
}

.residence_day {
  color: #40ab91;
}

/* 标题 */

.yz_box {
  width: 93%;
  margin: 0 auto;
  background: #fff;
  margin-bottom: 20rpx;
}

.yz_title {
  font-size: 40rpx;
  font-family: SourceHanSansCN-Medium;
  font-weight: 500;
  color: #222;
  line-height: 30px;
  margin-left: 13rpx;
}

.yz_jianJie {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
  font-size: 26rpx;
  font-family: SimSun;
  font-weight: 400;
  color: #5f5f5f;
  margin: 20rpx 13rpx;
}

.kong {
  width: 100%;
  height: 1rpx;
}

/* 综合评价 */

.liuyan {
  justify-content: space-between;
}

.liuyan view image {
  width: 31rpx;
  height: 30rpx;
}

.yz_pingjia {
  font-size: 20rpx;
  margin-top: 11rpx;
}

.yz_pingJia_title {
  margin: 2rpx 30rpx 0 30rpx;
}

.yz_title1 {
  justify-content: space-between;
}

.yz_title1 image {
  width: 36rpx;
  height: 39rpx;
  margin-top: 4rpx;
}

/* 留言内容 */

.name_info {
  justify-content: space-between;
  width: 96%;
  margin: 0 auto;
}

.name_info image {
  width: 50rpx;
  height: 50rpx;
}

.name_info .info_name {
  font-size: 36rpx;
  margin-left: 12rpx;
  color: #222;
}

.info_time {
  font-size: 26rpx;
  font-family: SimSun;
  font-weight: 400;
  color: rgba(34, 34, 34, 1);
  line-height: 70rpx;
}

.yz_info_content {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  font-size: 27rpx;
  font-family: SimSun;
  font-weight: 400;
  color: #5f5f5f;
  margin-left: 70rpx;
  margin-bottom: 20rpx;
}

/* 便利设施 */

.sheSi {
  font-size: 50rpx;
  line-height: 105rpx;
  color: #40ab91;
}

.yz_reserve {
  font-size: 24rpx;
  font-family: SimSun;
  font-weight: 400;
  color: #5f5f5f;
  margin-left: 13rpx;
  margin-top: 10rpx;
}

.yz_reserve>view {
  justify-content: space-between;
  line-height: 50rpx;
  padding-right: 5px;
}

.yz_reserve_title {
  font-size: 26rpx;
  font-family: SimSun;
  font-weight: 400;
  color: #5f5f5f;
  margin-left: 13rpx;
}

.yz_reserve_content {
  width: 70%;
  margin: 0 auto;
  padding-bottom: 10rpx;
  display: flex;
  flex-wrap: wrap;
}

.yz_reserve_content view {
  width: 210rpx;
  margin-left: 25rpx;
}

.yz_reserve_content view.active {
  color: #5f5f5f;
}

.yz_reserve_content image {
  width: 22rpx;
  height: 22rpx;
  margin-right: 11rpx;
}

/* 周边房源 */

.d_list {
  background: #f7f6f6;
  height: 100vh;
}

.d_img {
  width: 100%;
  height: 360rpx;
}

.d_img image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.d_good {
  margin-top: 11rpx;
}

.d_good>view {
  margin:0rpx 10rpx;
  border: 1px solid #888;
  height: 26rpx;
  font-family: SourceHanSansCN-Medium;
  font-weight: 500;
  border-radius: 5rpx;
  line-height: 16rpx;
}

.d_good .d_good_font {
  font-size: 20rpx;
  padding: 5rpx;
  color: #888;
  text-align: center;
}
.d_title {
  margin-top: 28rpx;
  margin-left: 10rpx;
}

.d_site_name {
  height:52rpx;
  font-size: 35rpx;
  color: #222;
}

.d_info {
  padding-top: 15rpx;
  margin-left:10rpx;
  width: 315px;
}

.d_info .d_fang {
  font-size: 24rpx;
}

.d_info .d_ren {
  font-size: 20rpx;
  margin-left: 55rpx;
  line-height:45rpx;
}

.d_info .d_fen {
  font-size: 22rpx;
  color: #40ab91;
  margin-left: 35rpx;
  line-height:45rpx;
}
.d_price1 {
  font-size: 30rpx;
  padding-bottom: 16rpx;
}

.d_price2 {
  font-size: 24rpx;
  margin-top: 5px;
  text-decoration: line-through;
  margin-left: 10rpx;
}

.d_vip {
  margin-left: 20rpx;
  font-size: 21rpx;
  color: #ffa500;
  border: 1px solid #ffdead;
  background: #ffdead;
  height: 31rpx;
  padding: 0rpx 10rpx;
  line-height: 30rpx;
  letter-spacing: 3rpx;
    margin-top:6rpx;
}


.d_yuding>view {
  font-size: 24rpx;
  text-align: center;
}

.yz_circum_box {
  white-space: nowrap;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  height: 666rpx;
}

.yz_circum_title {
  color: #40ab91;
  background: #f7f6f6;
}

.d_content {
  width: 100%;
  margin-left: 10px;
  background: #fff;
  height: 630rpx;
  box-shadow: 10px 10px 5px #888888;
}

.yz_price {
  width: 93%;
  margin: 0 auto;
  color: #5f5f5f;
  font-size: 24rpx;
}

.yz_price_a {
  flex-direction: column;
}

.yz_price1_a {
  margin-top: -104rpx;
  font-size: 26rpx;
  text-decoration: line-through;
}

.yz_price view {
  margin: 20rpx 0;
}

/* 底部 */

.yz_button {
  background: #40ab91;
  height: 138rpx;
  flex: 1;
  justify-content: space-around;
  color: #fff;
  line-height: 136rpx;
  font-size: 35rpx;
  text-align: center;
}

.yz_button image {
  background: #40ab91;
  width: 44rpx;
  height: 43rpx;
  position: relative;
  left: 15rpx;
  top: 10rpx;
}

.yz_taber {
  position: fixed;
  bottom: 0;
  width: 100%;
}

.yz_taber_top {
  height: 136rpx;
}

/* 客服 */

.button_server {
  background-color: #fff;
  border-radius: 0;
  height: 115rpx;
  line-height: 32rpx;
}

button::after {
  border: none;
}

/* 优惠卷 */

.coupon {
  border: 1px solid #40ab91;
  border-right: none;
  width: 95%;
  margin: 20rpx auto;
  height: 180rpx;
  color: #40ab91;
  justify-content: space-between;
  box-sizing:border-box;
}

.active {
  border: 1px solid #a4a4a4;
  border-right: none;
  width: 95%;
  margin: 20rpx auto;
  height: 180rpx;
  color: #a4a4a4;
  justify-content: space-between;
}

.coupon_price {
  font-size: 60rpx;
  font-family: FZCQJW--GB1-0;
  font-weight: bold;
}

.coupon_content>view:last-child {
  white-space: nowrap;
}

.coupon_price view:last-child {
  margin-left: 61rpx;
  margin-top: -25rpx;
}

.coupon_content {
  width: 30%;
  font-size: 18rpx;
  font-family: SimSun;
  font-weight: 400;
  height: 100rpx;
  margin-top: 10rpx;
}

.coupon_image {
  width: 33%;
  background: #fff;
}

.coupon_image image {
  width: 100%;
  height: 100%;
}

.yz_coupon_box {
  transition: 0.5s;
  width: 100%;
  height:1050rpx;
  position: fixed;
  background: #fff;
  z-index: 999;
  bottom: -1050rpx;
}

.yz_coupon_title {
  text-align: center;
  font-size: 40rpx;
  margin: 20rpx 0;
}

.show {
  transition: 0.5s;
  width: 100%;
  height:1050rpx;
  position: fixed;
  background: #fff;
  z-index: 999;
   bottom: 0rpx;
}

.yz_quding {
  width: 100%;
  height: 100rpx;
  position: fixed;
  z-index: 1000;
  color: #fff;
  background: #40ab91;
  bottom: 0;
  text-align: center;
  line-height: 100rpx;
  margin-bottom: -100rpx;
}
.yz_qudinga {
  width: 100%;
  height: 100rpx;
  position: fixed;
  z-index: 1000;
  color: #fff;
  background: #40ab91;
  bottom: 0;
  text-align: center;
  line-height: 100rpx;
}

/* 评分 */

.conmmentbox {
  display: inline-block;
  font-size: 22rpx;
  color: #f74754;
}

.conmmentstars {
  width: 26rpx;
  height: 26rpx;
}

.doortimes {
  color: #f74754;
  margin-left: 10rpx;
}

.starbox {
  width: 130rpx;
  height: 30rpx;
  position: relative;
  display: inline-block;
  overflow: hidden;
  float: left;
}

.stars {
  width: 130rpx;
  height: 30rpx;
  position: relative;
  top: 0;
  left: 0;
  overflow: hidden;
}

.stars2 {
  height: 30rpx;
  width: 130rpx;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
}

.stars image, .stars2 image {
  width: 26rpx;
  height: 26rpx;
  float: left;
  white-space: nowrap;
}

.stars view, .stars2 view {
  width: 130rpx;
  position: absolute;
  display: flex;
}

.star_box {
  width: 150rpx;
  height: 40rpx;
}

/* 地图 */

.mapImage {
  width: 100%;
  height: 244rpx;
}

.mapImage image {
  display: block;
  width: 100%;
  height: 100%;
}
