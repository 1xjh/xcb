page {
  height: 100%;
  background: #eee;
}

.many_hotel {
  background: #eee;
}

.box {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

.mycollect_box {
  width: 90%;
  margin: 0 auto;
  background: #fff;
}

.d_img {
  width: 100%;
  height: 360rpx;
}

.d_img image {
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
}

.d_good {
  margin-top: 40rpx;
}

.d_good>view {
  margin:0rpx 10rpx;
  border: 1px solid #888;
  height: 26rpx;
  font-family: SourceHanSansCN-Medium;
  font-weight: 500;
  border-radius: 5rpx;
  line-height: 16rpx;
}

.d_good .d_good_font {
  font-size: 20rpx;
  padding: 5rpx;
  color: #888;
  text-align: center;
}

.d_title {
  margin-top: 28rpx;
  height: 50rpx;
  margin-left: 10rpx;
}

.d_site_name {
  height:52rpx;
  font-size: 35rpx;
  color: #222;
}

.d_info {
  padding-top: 15rpx;
  margin-left:10rpx;
}

.d_info .d_fang {
  font-size: 26rpx;
}

.d_info .d_ren {
  font-size: 22rpx;
  margin-left: 20rpx;
}

.d_info .d_fen {
  font-size: 20rpx;
  color: #40ab91;
  margin-left: 35rpx;
}
.d_price1 {
  font-size: 30rpx;
  padding-bottom: 16rpx;
}

.d_price2 {
  font-size: 24rpx;
  margin-top: 5px;
  text-decoration: line-through;
  margin-left: 10rpx;
}
.d_vip {
  margin-left: 20rpx;
  font-size: 21rpx;
  color: #ffa500;
  border: 1px solid #ffdead;
  background: #ffdead;
  height: 31rpx;
  padding: 0rpx 10rpx;
  line-height: 30rpx;
  letter-spacing: 3rpx;
}

.d_yuding>view {
  font-size: 30rpx;
  text-align: center;
}

.d_bakcground {
  height: 25rpx;
  width: 100%;
  background: #eee;
}
