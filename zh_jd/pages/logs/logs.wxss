page {
  height: 100%;
  background: #efeff4;
}

.body {
  width: 90%;
  height: 600rpx;
  background: #fff;
  position: absolute;
  padding-bottom: 20rpx;
  border-radius: 10rpx;
  z-index: 2200;
}

.show {
  display: block;
  animation: zhuan 0.5s 1;
  animation-fill-mode: forwards;
}

.box {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-boxflex;
  display: flex;
}

@keyframes zhuan {
  0% {
    opacity: 0;
  }

  10% {
    opacity: 0.1;
  }

  20% {
    opacity: 0.2;
  }

  30% {
    opacity: 0.3;
  }

  40% {
    opacity: 0.4;
  }

  50% {
    opacity: 0.5;
  }

  60% {
    opacity: 0.6;
  }

  70% {
    opacity: 0.7;
  }

  80% {
    opacity: 0.8;
  }

  90% {
    opacity: 0.9;
  }

  100% {
    opacity: 1;
  }
}

.code {
  padding: 10rpx 23rpx;
  bottom: 20rpx;
  right: 10rpx;
  border-radius: 8rpx;
  font-size: 28rpx;
  color: #fff;
  background: #41ac8d;
}

.hidden {
  display: none;
  /* opacity: 0; */
  animation: zhuan2 0.5s 1;
}

@keyframes zhuan2 {
  0% {
    opacity: 1;
  }

  10% {
    opacity: 0.9;
  }

  20% {
    opacity: 0.8;
  }

  30% {
    opacity: 0.7;
  }

  40% {
    opacity: 0.6;
  }

  50% {
    opacity: 0.5;
  }

  60% {
    opacity: 0.4;
  }

  70% {
    opacity: 0.3;
  }

  80% {
    opacity: 0.2;
  }

  90% {
    opacity: 0.1;
  }

  100% {
    opacity: 0;
  }
}

/* .body>view:first-child{
  margin-top:120rpx;
} */

.body>view:nth-child(1) {
  line-height: 120rpx;
  font-size: 35rpx;
  text-align: center;
  border: none;
}

.body>view {
  width: 90%;
  height: 110rpx;
  font-size: 28rpx;
  /* line-height: 100rpx; */
  margin: 0 auto;
  border-bottom: 1rpx solid #ccc;
}

.body>view:last-child {
  border: none;
}

.body>view view:nth-child(1) {
  width: 40rpx;
  height: 40rpx;
}

.body>view view:nth-child(2) {
  width: 500rpx;
  height: 100%;
  left: 60rpx;
}

.body>view view:nth-child(3) {
  font-size: 26rpx;
  line-height: 50rpx;
  border-radius: 8rpx;
  text-align: center;
}

.body>view view input {
  width: 80%;
  height: 100%;
  font-size: 28rpx;
  color: #000;
}

.lgbut {
  width: 80%;
  height: 80rpx;
  line-height: 80rpx;
  background: #41ac8d;
  color: #fff;
  margin-top: 50rpx;
}

.title {
  width: 500rpx;
  height: 60rpx;
  position: fixed;
  line-height: 60rpx;
  background: orange;
  z-index: 111;
  text-align: center;
  font-size: 26rpx;
}

.validate {
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 2000;
}

.back {
  width: 100%;
  height: 100%;
  background: #000;
  z-index: 2100;
  opacity: 0.5;
}

.top {
  height: 360rpx;
  width: 100%;
  position: relative;
}

.top>image {
  height: 360rpx;
  width: 100%;
}

.layer {
  width: 95%;
  height: 200rpx;
  background: #fff;
  z-index: 10;
  bottom: -20rpx;
  border-radius: 10rpx;
}

.classification {
  width: 100%;
  height: 150rpx;
  display: flex;
}

.classification>navigator {
  flex: 3;
  text-align: center;
}

.classification>navigator .child {
  color: #666;
  font-size: 28rpx;
}

.user_image {
  width: 160rpx;
  height: 160rpx;
  background-color: black;
  border-radius: 50%;
  z-index: 1000;
  margin-top: -80rpx;
}

.middle {
  height: 170rpx;
  width: 100%;
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.middle_in {
  width: 250rpx;
  height: 104rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  font-size: 30rpx;
  color: #666;
}

.middle_in button {
  width: auto;
  height: auto;
  margin: 0;
  padding: 0;
  margin-top: 10rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  background: #fff;
  color: #666;
}

.middle_in button::after {
  border: none;
}

.middle image {
  height: 40rpx;
  width: 40rpx;
}

.logs1 {
  color: #333;
  font-size: 32rpx;
}

.logs2 {
  font-size: 30rpx;
}

.support {
  display: flex;
  width: 100%;
  align-items: center;
  justify-content: center;
  height: 400rpx;
  font-size: 30rpx;
  color: #999;
}

.support image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
  border-radius: 6rpx;
  overflow: hidden;
}

.users {
  width: 100%;
  height: 230rpx;
  position: absolute;
  top: 50rpx;
}

.users>view:nth-child(1) {
  width: 100%;
  height: 150rpx;
}

.users>view:nth-child(2) {
  width: 100%;
  /* height: 60rpx; */
  color: #fff;
  /* line-height: 60rpx; */
  text-align: center;
  font-size: 30rpx;
}

.member {
  width: 100%;
  height: 80rpx;
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
}

.users>view image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}

.mine {
  height: auto;
  /* margin-top:200rpx; */
  background: #fff;
  margin: 5px auto 0;
  border-radius: 15rpx;
}

.mine>view {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #222;
  border-bottom: 1rpx solid #f4f4f4;
}

.mine>view text {
  margin-left: 80rpx;
  /* display: inline-block; */
}

.img2 {
  right: 20rpx;
  width: 25rpx;
  height: 40rpx;
}

.img3 {
  width: 40rpx;
  height: 40rpx;
  left: 20rpx;
}

.member-head {
  height: 260rpx;
  background: #41ac8d;
  padding: 30rpx 0;
  position: relative;
}

.member-head .child {
  width: 30%;
  text-align: center;
}

.member-head .child.userinfo {
  width: 100%;
  color: #fff;
  padding-top: 20rpx;
}

.member-head .child.userinfo .avatar {
  height: 140rpx;
  width: 140rpx;
  background: #fff;
  margin: auto;
  border-radius: 96rpx;
  border: 4rpx solid #fff;
}

.member-head .child.userinfo .avatar wx-image {
  height: 140rpx;
  width: 140rpx;
  border-radius: 96rpx;
  display: block;
}

/* 姓名 */

.member-head .child.userinfo .nickname {
  height: 55rpx;
  padding-top: 8rpx;
  font-size: 24rpx;
  color: inherit;
  text-align: center;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  margin-right: 20rpx;
  padding: 10rpx 0;
}

/* 会员 */

.member-head .child.userinfo .level {
  font-size: 30rpx;
  color: inherit;
  text-align: center;
  margin-left: 50rpx;
}

.member-head .child .name {
  padding-top: 48rpx;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
}

.member-head .child .number {
  font-size: 28rpx;
  color: #ed0a15;
  text-align: center;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.member-head .child .btn {
  width: 104rpx;
  height: 40rpx;
  padding: 0;
  margin: 8rpx auto 0;
  color: #fff;
  font-size: 24rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 40rpx;
  border: 2rpx solid #fff;
}

/* 收藏积分 */

.integral {
  width: 80%;
  background: #fff;
  position: absolute;
  height: 88rpx;
  margin-left: 77rpx;
  border-radius: 20rpx 20rpx 0 0;
}

.integral_background {
  height: 66rpx;
}

.integral_left, .integral_right {
  width: 50%;
  height: 88rpx;
  line-height: 66rpx;
}

.integral_left {
  border-right: 1px solid #eee;
}

.left_image, .right_image {
  width: 48rpx;
  height: 47rpx;
  margin-left: 20rpx;
  margin-top: 19rpx;
  position: absolute;
}

.integral image {
  width: 100%;
  height: 100%;
}

.integral text {
  text-align: center;
  line-height: 88rpx;
  width: 100%;
  height: 88rpx;
  padding-left: 40rpx;
}

/* 姓名 */

.logs_name {
  width: 100;
  justify-content: space-around;
  vertical-align: middle;
}

/* 客服 */

.button {
  width: 100%;
  height: 80rpx;
  font-size: 30rpx;
  color: #222;
  border-bottom: 1rpx solid #f4f4f4;
  padding: 0 0;
  display: flex;
  background: none;
  border: none;
  border-radius: 0;
}

.button::after {
  border: none;
}

input {
  outline: none;
  border: none;
  list-style: none;
}

/* 用户登陆 */

.login {
  width: 100%;
  height: 55rpx;
  font-size: 35rpx;
  color: #fff;
  border-bottom: 1rpx solid #f4f4f4;
  padding: 0 0;
  display: flex;
  background: none;
  border: 1px solid #fff;
  border-radius: 10%;
  line-height: 45rpx;
  text-indent: 35rpx;
}

.login::after {
